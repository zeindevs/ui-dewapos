    "gulp-rimraf": "^1.0.0",
    "gulp-rollup": "^2.17.0",
    "gulp-sass": "^5.1.0",
    "prettier": "^3.2.4",
    "prettier-plugin-ejs": "^1.0.3",
    "rollup": "^4.9.6",
    "sass": "^1.70.0"
  }
